{
  "name": "03_sample_augmentation",
  "description": "",
  "pipeline": [
    {
      "class": "sklearn.preprocessing._data.StandardScaler"
    },
    {
      "y_processing": {
        "class": "sklearn.preprocessing._data.RobustScaler"
      }
    },
    {
      "sample_augmentation": {
        "transformers": [
          {
            "class": "nirs4all.operators.augmentation.random.Rotate_Translate",
            "params": {
              "p_range": 1.0,
              "y_factor": 2.0
            }
          },
          {
            "class": "nirs4all.operators.augmentation.spectral.GaussianAdditiveNoise",
            "params": {
              "sigma": 0.005
            }
          },
          {
            "class": "nirs4all.operators.augmentation.spectral.MultiplicativeNoise",
            "params": {
              "sigma_gain": 0.02
            }
          },
          {
            "class": "nirs4all.operators.augmentation.spectral.LinearBaselineDrift",
            "params": {
              "offset_range": [
                -0.05,
                0.05
              ],
              "slope_range": [
                -0.001,
                0.001
              ]
            }
          },
          {
            "class": "nirs4all.operators.augmentation.spectral.WavelengthShift",
            "params": {
              "shift_range": [
                -2.0,
                2.0
              ]
            }
          }
        ],
        "count": 3,
        "selection": "random",
        "random_state": 42
      }
    },
    {
      "class": "sklearn.model_selection._split.ShuffleSplit",
      "params": {
        "n_splits": 3,
        "test_size": 0.2,
        "random_state": 42
      }
    },
    {
      "class": "nirs4all.operators.transforms.scalers.StandardNormalVariate"
    },
    {
      "model": {
        "class": "sklearn.cross_decomposition._pls.PLSRegression",
        "params": {
          "n_components": 10
        }
      },
      "name": "PLS-Augmented"
    },
    {
      "model": {
        "class": "sklearn.ensemble._forest.RandomForestRegressor",
        "params": {
          "n_estimators": 50,
          "max_depth": 10,
          "random_state": 42
        }
      },
      "name": "RF-Augmented"
    }
  ],
  "has_generators": false,
  "num_configurations": 1
}