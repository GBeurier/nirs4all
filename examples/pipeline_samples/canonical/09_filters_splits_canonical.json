{
  "name": "09_filters_splits",
  "description": "",
  "pipeline": [
    "chart_2d",
    {
      "sample_filter": {
        "filters": [
          {
            "class": "nirs4all.operators.filters.y_outlier.YOutlierFilter",
            "params": {
              "method": "iqr",
              "threshold": 1.5,
              "reason": "iqr_outlier"
            }
          },
          {
            "class": "nirs4all.operators.filters.spectral_quality.SpectralQualityFilter",
            "params": {
              "max_nan_ratio": 0.1,
              "max_zero_ratio": 0.3,
              "min_variance": 1e-06,
              "max_value": 4.0,
              "min_value": -0.5
            }
          }
        ],
        "mode": "any",
        "report": true
      }
    },
    {
      "chart_2d": {
        "include_excluded": true,
        "highlight_excluded": true
      }
    },
    {
      "class": "sklearn.preprocessing._data.MinMaxScaler"
    },
    {
      "y_processing": {
        "class": "sklearn.preprocessing._data.StandardScaler"
      }
    },
    {
      "class": "nirs4all.operators.splitters.splitters.SPXYGFold",
      "params": {
        "n_splits": 1,
        "test_size": 0.2,
        "random_state": 42
      }
    },
    {
      "class": "sklearn.model_selection._split.KFold",
      "params": {
        "n_splits": 3,
        "shuffle": true,
        "random_state": 42
      }
    },
    {
      "class": "nirs4all.operators.transforms.scalers.StandardNormalVariate"
    },
    {
      "model": {
        "class": "sklearn.cross_decomposition._pls.PLSRegression",
        "params": {
          "n_components": 10
        }
      },
      "name": "PLS-Filtered"
    },
    {
      "model": {
        "class": "sklearn.ensemble._forest.RandomForestRegressor",
        "params": {
          "n_estimators": 100,
          "max_depth": 10,
          "random_state": 42
        }
      },
      "name": "RF-Filtered"
    }
  ],
  "has_generators": false,
  "num_configurations": 1
}